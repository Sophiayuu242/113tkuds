import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int N = scn.nextInt();

        int result = 1;
        int i = 1;
        while (i <= N) {
            result *= i;
            i++;
        }

        System.out.println(result);
    }
}
