import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int Y = scn.nextInt();

        if ((Y % 400 == 0) || (Y % 4 == 0 && Y % 100 != 0)) {
            System.out.println("Yes");
        } else {
            System.out.println("No");
        }
    }
}
